{
  "version" : 2,
  "builds":[
    {
      "src":"server.js",
      "use":"@vercel/node"
    },
    { "src": "/*.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/*.html", "dest": "./public" },
    { "src": "/sms", "methods": ["POST"], "dest": "/api/sms" },
    { "src": "/(.*)", "dest": "server.js" }
  ]
}